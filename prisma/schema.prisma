datasource db {
  provider = "postgres"
  // url      = "postgres://testPsqlUser:yourpassword@localhost:6666/dockerdatabase?schema=test"
  url      = env("DATABASE_URL_WITH_SCHEMA")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
}
